{
  "main": {
    "tag": "div",
    "class": "manager"
  },

  "userstory": {
    "tag": "div",
    "class": "userstory",
    "inner": [
      {
        "tag": "div",
        "class": "key",
        "inner": "%key%"
      },
      {
        "tag": "div",
        "class": "headline",
        "inner": "%headline%"
      },
      {
        "tag": "div",
        "class": "name",
        "inner": "%name%"
      },
      {
        "tag": "div",
        "class": "description",
        "inner": "%description%"
      },
      {
        "tag": "div",
        "class": "effort",
        "inner": "%effort%"
      },
      {
        "tag": "div",
        "class": "wert",
        "inner": "%wert%"
      },
      {
        "tag": "div",
        "class": "priority",
        "inner": "%priority%"
      }
    ]
  },

  "input": {
    "tag": "div",
    "class": "new_userstory",
    "inner":[
      {
        "tag": "div",
        "class": "name"
      },
      {
        "tag":"div",
        "class":"inhalt",
        "inner":{
          "tag": "form",
          "inner":[
            {
              "tag":"div",
              "class":"row",
              "inner":[
                {
                  "tag":"div",
                  "class":"definer",
                  "inner":"Headline"
                },
                {
                  "tag": "input",
                  "id":"headline",
                  "placeholder": "Enter headline here..."
                }
              ]
            },
            {
              "tag":"div",
              "class":"definer",
              "inner":"Description"
            },
            {
              "tag":"div",
              "class":"row",
              "inner":[
                {
                  "tag": "textarea",
                  "id":"description",
                  "placeholder": "Enter description here..."
                }
              ]
            },
            {
              "tag":"div",
              "class":"definer",
              "inner":"Effort"
            },
            {
              "tag":"div",
              "class":"row",
              "inner":[
                {
                  "tag": "input",
                  "id":"effort",
                  "type": "range",
                  "min":"1",
                  "max":"5"
                },
                {
                  "tag":"span",
                  "id":"counteff",
                  "value":"effcounter"
                }
              ]
            },
            {
              "tag":"div",
              "class":"definer",
              "inner":"Value"
            },
            {
              "tag":"div",
              "class":"row",
              "inner":[
                {
                  "tag": "input",
                  "id":"wert",
                  "type": "range",
                  "min":"1",
                  "max":"5"
                }
              ]
            },
            {
              "tag":"div",
              "class":"definer",
              "inner":"Priority"
            },
            {
              "tag":"div",
              "class":"row",
              "inner":[
                {
                  "tag": "input",
                  "id":"priority",
                  "type": "range",
                  "min":"1",
                  "max":"5"
                }
              ]
            },
            {
              "tag":"div",
              "class":"row",
              "inner":[
                {
                  "tag": "input",
                  "type": "submit",
                  "id":"submitus",
                  "value": "Save Userstory"
                }
              ]
            }
          ],
          "onsubmit": "%onsubmit%"
        }
      }
    ]
  },
  "deleteus": {
    "tag":"div",
    "class":"delete_userstory",
    "inner":[
      {
        "tag": "div",
        "class": "inhalt",
        "inner": {
          "tag": "form",
          "inner": [
            {
              "tag": "input",
              "id": "us-delete-id",
              "placeholder": "delete user story id"
            },
            {
              "tag": "input",
              "type": "submit",
              "id":"searchus",
              "onclick":"%onsubmit%",
              "value": "Delete Userstory"
            }
          ]
        }
      }
    ]
  }
}