{
  "main": {
    "tag": "div",
    "class": "manager"
  },

  "userstory": {
    "tag": "div",
    "class": "userstory",
    "inner": [
      {
        "tag": "div",
        "class": "headline",
        "inner": "%headline%"
      },
      {
        "tag": "div",
        "class": "name",
        "inner": "%name%"
      },
      {
        "tag": "div",
        "class": "description",
        "inner": "%description%"
      },
      {
        "tag": "div",
        "class": "effort",
        "inner": "%effort%"
      },
      {
        "tag": "div",
        "class": "wert",
        "inner": "%wert%"
      },
      {
        "tag": "div",
        "class": "priority",
        "inner": "%priority%"
      }
    ]
  },

  "input": {
    "tag": "div",
    "class": "new_userstory",
    "inner":[
      {
        "tag": "div",
        "class": "name"
      },
      {
        "tag":"div",
        "class":"inhalt",
        "inner":{
          "tag": "form",
          "inner":[
            {
              "tag":"div",
              "class":"row",
              "inner":[
                {
                  "tag": "input",
                  "id":"headline",
                  "placeholder": "Enter headline here..."
                }
              ]
            },
            {
              "tag":"div",
              "class":"row",
              "id":"description",
              "inner":[
                {
                  "tag": "input",
                  "placeholder": "Enter description here..."
                }
              ]
            },
            {
              "tag":"div",
              "class":"row",
              "id":"effort",
              "inner":[
                {
                  "tag": "input",
                  "placeholder": "Enter effort here..."
                }
              ]
            },
            {
              "tag":"div",
              "class":"row",
              "id":"wert",
              "inner":[
                {
                  "tag": "input",
                  "placeholder": "Enter value here..."
                }
              ]
            },
            {
              "tag":"div",
              "class":"row",
              "id":"priority",
              "inner":[
                {
                  "tag": "input",
                  "placeholder": "Enter priority here..."
                }
              ]
            },
            {
              "tag":"div",
              "class":"row",
              "inner":[
                {
                  "tag": "input",
                  "type": "submit",
                  "id":"submitus",
                  "value": "Send"
                }
              ]
            }
          ],
          "onsubmit": "%onsubmit%"
        }
      }
    ]
  },
  "sucheus": {
    "tag":"div",
    "class":"suche_userstory",
    "inner":[
      {
        "tag": "div",
        "class": "inhalt",
        "inner": {
          "tag": "form",
          "inner": [
            {
              "tag": "input",
              "placeholder": "search for userstory headline..."
            },
            {
              "tag": "input",
              "type": "submit",
              "id":"searchus",
              "onclick":"%onclick%",
              "value": "Send"
            }
          ]
        }
      }
    ]
  }
}