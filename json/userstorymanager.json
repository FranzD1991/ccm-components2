{
  "main": {
    "tag": "div",
    "class": "manager"
  },

  "userstory": {
    "tag": "div",
    "class": "userstory",
    "inner": [
      {
        "tag":"div",
        "class":"srow",
        "inner":[
          {
            "tag":"span",
            "class":"keydis",
            "inner":"Key:"
          },
          {
            "tag": "span",
            "class": "key",
            "inner": "%key%"
          }
        ]
      },
      {
        "tag":"div",
        "class":"srow",
        "inner":[
          {
            "tag":"span",
            "class":"keydis",
            "inner":"Headline:"
          },
          {
            "tag": "span",
            "class": "headline",
            "inner": "%headline%"
          }
        ]
      },
      {
        "tag":"div",
        "class":"srow",
        "inner":[
          {
            "tag":"span",
            "class":"keydis",
            "inner":"Created by:"
          },
          {
            "tag": "span",
            "class": "name",
            "inner": "%name%"
          }
        ]
      },
      {
        "tag":"div",
        "class":"row",
        "inner":[
          {
            "tag":"span",
            "class":"descriptionhead",
            "inner":"Description:"
          },
          {
            "tag": "div",
            "class": "description",
            "inner": "%description%"
          }
        ]
      },
      {
        "tag":"div",
        "class":"brow",
        "inner":[
          {
            "tag":"div",
            "class":"keydis",
            "inner":"Effort:"
          },
          {
            "tag": "div",
            "class": "effort",
            "inner": "%effort%"
          },
          {
            "tag":"div",
            "class":"keydis",
            "inner":"Value:"
          },
          {
            "tag": "div",
            "class": "wert",
            "inner": "%wert%"
          },
          {
            "tag":"div",
            "class":"keydis",
            "inner":"Priority:"
          },
          {
            "tag": "div",
            "class": "priority",
            "inner": "%priority%"
          }
        ]
      }
    ]
  },

  "input": {
    "tag": "div",
    "class": "new_userstory",
    "inner":[
      {
        "tag": "div",
        "class": "name"
      },
      {
        "tag":"div",
        "class":"inhalt",
        "inner":{
          "tag": "form",
          "inner":[
            {
              "tag":"div",
              "class":"row",
              "inner":[
                {
                  "tag":"div",
                  "class":"definer",
                  "inner":"Headline"
                },
                {
                  "tag": "input",
                  "id":"headline",
                  "class":"longfield",
                  "placeholder": "Enter headline here..."
                }
              ]
            },
            {
              "tag":"div",
              "class":"definer",
              "inner":"Description"
            },
            {
              "tag":"div",
              "class":"row",
              "inner":[
                {
                  "tag": "textarea",
                  "class":"longarea",
                  "id":"description",
                  "placeholder": "Enter description here..."
                }
              ]
            },
            {
              "tag":"div",
              "class":"definer",
              "inner":"Effort"
            },
            {
              "tag":"div",
              "class":"row",
              "inner":[
                {
                  "tag": "input",
                  "id":"effort",
                  "type": "range",
                  "class":"slider",
                  "min":"1",
                  "max":"5"
                },
                {
                  "tag":"span",
                  "class":"counter",
                  "id":"count_effort",
                  "inner":"5"
                }
              ]
            },
            {
              "tag":"div",
              "class":"definer",
              "inner":"Value"
            },
            {
              "tag":"div",
              "class":"row",
              "inner":[
                {
                  "tag": "input",
                  "id":"wert",
                  "type": "range",
                  "class":"slider",
                  "min":"1",
                  "max":"5"
                },
                {
                  "tag":"span",
                  "class":"counter",
                  "id":"count_wert",
                  "inner":"5"
                }
              ]
            },
            {
              "tag":"div",
              "class":"definer",
              "inner":"Priority"
            },
            {
              "tag":"div",
              "class":"row",
              "inner":[
                {
                  "tag": "input",
                  "id":"priority",
                  "type": "range",
                  "class":"slider",
                  "min":"1",
                  "max":"5"
                },
                {
                  "tag":"span",
                  "class":"counter",
                  "id":"count_priority",
                  "inner":"5"
                }
              ]
            },
            {
              "tag":"div",
              "class":"row",
              "inner":[
                {
                  "tag": "input",
                  "type": "submit",
                  "class":"menubutton",
                  "id":"submitus",
                  "value": "Save Userstory"
                }
              ]
            }
          ],
          "onchange": "%onchange%",
          "onsubmit": "%onsubmit%"
        }
      }
    ]
  },
  "deleteus": {
    "tag":"div",
    "class":"delete_userstory",
    "inner":[
      {
        "tag": "div",
        "class": "inhalt",
        "inner": {
          "tag": "form",
          "inner": [
            {
              "tag": "input",
              "id": "us-delete-id",
              "class":"littlefield",
              "placeholder": "Delete ID"
            },
            {
              "tag": "input",
              "type": "submit",
              "class":"littlebutton",
              "id":"searchus",
              "onclick":"%onclick%",
              "value": "Delete Userstory"
            }
          ]
        }
      }
    ]
  },
  "clearus":{
    "tag":"div",
    "class":"zusatzfunktion",
    "inner":[
      {
        "tag":"input",
        "type":"submit",
        "class":"menubutton",
        "id":"deleteallus",
        "onclick":"%onclick%",
        "value":"Delete all Userstories"
      }
    ]
  },
  "sortini":{
    "tag":"div",
    "class":"zusatzfunktion",
    "inner":"Sort Userstories according to:"
  },
  "sortwer":{
    "tag":"div",
    "class":"zusatzfunktion",
    "inner":[
      {
        "tag":"input",
        "type":"submit",
        "class":"menubutton",
        "id":"sortwerte",
        "onclick":"%onclick%",
        "value":"Value"
      }
    ]
  },
  "sortprio":{
    "tag":"div",
    "class":"zusatzfunktion",
    "inner":[
      {
        "tag":"input",
        "type":"submit",
        "class":"menubutton",
        "id":"sortpriority",
        "onclick":"%onclick%",
        "value":"Priority"
      }
    ]
  },
  "sorteff":{
    "tag":"div",
    "class":"zusatzfunktion",
    "inner":[
      {
        "tag":"input",
        "type":"submit",
        "class":"menubutton",
        "id":"sorteffort",
        "onclick":"%onclick%",
        "value":"Effort"
      }
    ]
  }
}